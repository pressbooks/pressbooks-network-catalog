{
    "name": "pressbooks/pressbooks-network-catalog",
    "description": "Add a SPA-like catalog to the Pressbooks Aldine theme",
    "type": "wordpress-plugin",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Pressbooks (Book Oven Inc.)",
            "email": "code@pressbooks.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4",
        "pressbooks/pressbooks": "^6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.10"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "autoload": {
        "psr-4": {
            "PressbooksNetworkCatalog\\": "src/"
        }
    },
    "scripts": {
        "fix": [
            "vendor/bin/php-cs-fixer fix"
        ]
    }
}
